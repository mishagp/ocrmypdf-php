{
  "name": "mishagp/ocrmypdf",
  "description": "A simple PHP wrapper for OCRmyPDF",
  "type": "library",
  "keywords": [
    "OCRmyPDF",
    "wrapper",
    "ocr",
    "php"
  ],
  "license": "AGPL-3.0-only",
  "authors": [
    {
      "name": "mishagp",
      "email": "misha@mishaprasolov.com"
    }
  ],
  "support": {
    "issues": "https://github.com/mishagp/ocrmypdf-php/issues",
    "source": "https://github.com/mishagp/ocrmypdf-php"
  },
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/php-code-coverage": "^11.0.0",
    "phpunit/phpunit": "^11.0",
    "phpstan/phpstan": "^2.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-strict-rules": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "mishagp\\OCRmyPDF\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mishagp\\OCRmyPDF\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
